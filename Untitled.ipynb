     "name": "stdout",
     "text": "Eq(x(t), C2*t + C1 + O(t**2))\n"
    }
   ],
   "source": [
    "x=Function('x')\n",
    "a0=symbols('a0')\n",
    "a1=symbols('a1')\n",
    "ODE2=x(t).diff(t,2)-t*x(t)\n",
    "print(dsolve(ODE2, hint='2nd_power_series_ordinary', n=2 ,ics={x(0):a0,x(t).diff(t).subs(t,0):a1}))"
   ]
